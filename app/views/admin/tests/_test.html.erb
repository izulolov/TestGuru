  <tr>
    <td><%= test.title %></td>
    <td><%= test_level(test) %></td>
    <td class="center"><%= test_questions_number(test) %></td>
    <% if user_signed_in? %>
    <td><%= link_to 'Show', admin_test_path(test) %></td>
    <td><%= link_to 'Edit', edit_admin_test_path(test) %></td>
    <td>
    <%= link_to 'Delete',
                admin_test_path(test),
                method: :delete,
                data: { confirm: 'Are you sure?'} %>
    </td>
    <td><%= button_to 'Start', start_test_path(test) %></td>
    <% else %>
      <td><%= button_to 'Start', start_test_path(test), data: { confirm: 'Please log in first'} %></td>
    <% end %>
  </tr>
